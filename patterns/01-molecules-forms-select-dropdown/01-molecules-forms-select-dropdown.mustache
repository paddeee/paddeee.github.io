{{# selectItem }}
<div class="c-field">
  <select id="{{ id }}">
        <optgroup label="{{ label }}">
            {{#options }}
                <option>{{.}}</option>
            {{/options }}
        </optgroup>
    </select>
  <span role="status" aria-live="polite" class="is-vishidden"></span> {{# fieldNote }}
  <span class="c-field__note">{{{ fieldNote }}}</span> {{/ fieldNote }}

</div>
<!--end c-field-->
{{/ selectItem }}